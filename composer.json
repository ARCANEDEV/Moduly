{
    "name": "arcanedev/moduly",
    "description": "Modules manager for ARCANESOFT",
    "keywords": ["arcanesoft", "modules"],
    "homepage": "https://github.com/ARCANEDEV/Moduly",
    "license": "MIT",
    "authors": [
        {
            "name": "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANEDEV"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "arcanedev/composer": "~1.0",
        "arcanedev/support": "~2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1.0",
        "phpunit/phpunit": "~4.0",
        "phpunit/phpcov": "~2.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Moduly\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Moduly\\Tests\\": "tests/",
            "Arcanedev\\Foo\\": "tests/fixtures/modules/foo/src/",
            "Arcanedev\\Bar\\": "tests/fixtures/modules/bar/src/",
            "Arcanedev\\Baz\\": "tests/fixtures/modules/baz/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
